<% columns ||= list_columns -%>
<tr <%= as_element_attributes :list_calculations, id: active_scaffold_calculations_id, class: 'active-scaffold-calculations' %>>
  <% columns.each do |column| -%>
    <% if column.calculation? %>
    <td id="<%= active_scaffold_calculations_id(column: column) %>"><%= render_column_calculation(column) %></td>
    <% else %>
    <td>&nbsp;</td>
    <% end -%>
  <% end -%>
  <% unless active_scaffold_config.action_links.empty? || grouped_search? -%>
    <td class="actions">&nbsp;</td>
  <% end -%>
</tr>
